{
  "name": "@einride/eslint-plugin",
  "version": "0.0.0-development",
  "description": "Einride's ESLint configuration as a sharable plugin",
  "scripts": {
    "commitlint": "commitlint --from origin/main --to HEAD",
    "git-verify-nodiff": "./scripts/git-verify-nodiff.bash",
    "prettier": "prettier --write .",
    "review": "yarn install --frozen-lockfile && yarn commitlint && yarn prettier && yarn git-verify-nodiff"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "15.0.0",
    "commitlint": "15.0.0",
    "prettier": "2.5.1",
    "semantic-release": "18.0.1"
  },
  "peerDependencies": {
    "eslint": ">=7.28.0",
    "prettier": "^2.4.1"
  },
  "author": "Filip Tammerg√•rd",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "react",
    "typescript",
    "einride",
    "styleguide"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/eslint-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/eslint-plugin.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/eslint-plugin/issues"
  }
}
