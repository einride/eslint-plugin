  "engines": {
    "node": "14 || 16"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/eslint-plugin/issues"
  },
  "homepage": "https://github.com/einride/eslint-plugin"
