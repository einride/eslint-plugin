{
  "name": "@einride/eslint-plugin",
  "version": "0.0.0-development",
  "description": "Einride's default ESLint config",
  "main": "index.js",
  "scripts": {
    "commitlint": "commitlint --from origin/main --to HEAD",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "review": "yarn install --frozen-lockfile && yarn commitlint && yarn format-check"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@einride/prettier-config": "2.0.0",
    "eslint": "8.18.0",
    "prettier": "2.7.1",
    "semantic-release": "19.0.3"
  },
  "peerDependencies": {
    "eslint": ">=7.28.0",
    "prettier": "^2.4.1"
  },
  "files": [
    "index.js"
  ],
